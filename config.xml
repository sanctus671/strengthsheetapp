<?xml version='1.0' encoding='utf-8'?>
<widget id="com.strengthsheet.app" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <gap:config-file parent="/manifest" platform="android">
        <application android:theme="@android:style/Theme.Material.Light.NoActionBar" />
    </gap:config-file>
    <name>StrengthSheet</name>
    <description>StrengthSheet client tracking and messaging app.</description>
    <author email="taylordonhamling@gmail.com" href="http://www.strengthsheet.com">Taylor Hamling</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-6.5.0" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenBackgroundColor" value="0xFFFFFFFF" />
    <platform name="android">
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="StatusBarBackgroundColor" value="#173e19" />
        <allow-intent href="market:*" />
        <preference name="loadUrlTimeoutValue" value="700000" />
        <icon density="ldpi" src="resources/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="resources/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="resources/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="resources/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="resources/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/android/icon/drawable-xxxhdpi-icon.png" />
    </platform>
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="resources/android/splash/splash-port-ldpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-mdpi" src="resources/android/splash/splash-port-mdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="resources/android/splash/splash-port-hdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="resources/android/splash/splash-port-xhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xxhdpi" src="resources/android/splash/splash-port-xxhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xxxhdpi" src="resources/android/splash/splash-port-xxxhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-ldpi" src="resources/android/splash/splash-land-ldpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-mdpi" src="resources/android/splash/splash-land-mdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-hdpi" src="resources/android/splash/splash-land-hdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-xhdpi" src="resources/android/splash/splash-land-xhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-xxhdpi" src="resources/android/splash/splash-land-xxhdpi.png" />
    <gap:splash gap:platform="android" gap:qualifier="land-xxxhdpi" src="resources/android/splash/splash-land-xxxhdpi.png" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="ios-statusbarstyle" value="black-opaque" />
        <icon height="1024" src="resources/ios/icon/icon-1024.png" width="1024" />
        <icon height="29" src="resources/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="resources/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/icon-small@3x.png" width="87" />
        <icon height="40" src="resources/ios/icon/icon-small-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/icon-small-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/icon-small-40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/icon-small-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon-small-50@2x.png" width="100" />
        <icon height="57" src="resources/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/icon@2x.png" width="114" />
        <icon height="60" src="resources/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/icon-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/icon-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/icon-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/icon-167.png" width="167" />
        <icon height="167" src="resources/ios/icon/icon-83.5@2x.png" width="167" />
    </platform>
    <gap:splash gap:platform="ios" height="480" src="resources/ios/splash/Default~iphone.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="resources/ios/splash/Default@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="resources/ios/splash/Default-Portrait~ipad.png" width="768" />
    <gap:splash gap:platform="ios" height="2048" src="resources/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
    <gap:splash gap:platform="ios" height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="1334" src="resources/ios/splash/Default-667h.png" width="750" />
    <gap:splash gap:platform="ios" height="2208" src="resources/ios/splash/Default-736h.png" width="1242" />
    <gap:splash gap:platform="ios" height="768" src="resources/ios/splash/Default-Landscape~ipad.png" width="1024" />
    <gap:splash gap:platform="ios" height="1536" src="resources/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@2x~iphone~anyany.png" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@2x~iphone~comany.png" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@3x~iphone~anyany.png" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@3x~iphone~comany.png" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@2x~ipad~anyany.png" />
    <gap:splash gap:platform="ios" src="resources/ios/splash/Default@2x~ipad~comany.png" />
    
    <icon src="resources/icon.png" />
    <gap:splash src="resources/splash.png" />
    <engine name="android" spec="~6.3.0" />    
    
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <gap:plugin spec="https://github.com/brodybits/cordova-sqlite-legacy-build-support" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <gap:plugin spec="https://github.com/pdyraga/cordova-plugin-splashscreen" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.16" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <gap:plugin spec="https://github.com/hypery2k/cordova-email-plugin#master" />
    <gap:plugin name="cordova-plugin-file" source="npm" version="^4.3.3" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" version="^2.3.0" />
    <gap:plugin name="cordova-plugin-file-transfer" source="npm" version="1.5.1" />
    <gap:plugin name="cordova-plugin-camera" source="npm" version="^2.4.1">
        <param name="CAMERA_USAGE_DESCRIPTION" value="We are using the Camera to take pictures for an avatar" />
        <param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="We are using the Photo Library to get pictures for an avatar" />
    </gap:plugin>
    <plugin name="onesignal-cordova-plugin" spec="^2.1.0" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />    
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
</widget>